# Xcode environment configuration for React Native
# This file is used by Xcode to set up the environment for building

# Add Homebrew and rbenv to PATH
export PATH="/opt/homebrew/bin:$PATH"

# Initialize rbenv
if command -v rbenv >/dev/null 2>&1; then
  eval "$(rbenv init -)"
fi

# Ensure Node.js is available
export NODE_BINARY="/opt/homebrew/bin/node"